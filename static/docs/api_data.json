[  {    "type": "get",    "url": "/api/convert",    "title": "Convert a file",    "name": "Convert",    "group": "Convert",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "from",            "description": "<p>Format to convert from.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "to",            "description": "<p>Format to convert to.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "url",            "description": "<p>URL for the file to convert.</p> "          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "url",            "description": "<p>URL for the file to convert.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "from",            "description": "<p>Format to convert from.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "to",            "description": "<p>Format to convert to.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "output",            "description": "<p>The output of the conversion.</p> "          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  \"url\": \"http://example.com/my.pdf\",\n  \"from\": \"pdf\",\n  \"to\": \"html\",\n  \"output\": \"<!DOCTYPE html><html><head></head><body><h1>Example PDF</h1></body></html>\"\n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "InvalidURL",            "description": "<p>Please specify a valid URL</p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "InvalidFromFormat",            "description": "<p>Please specify a format to convert from</p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "InvalidToFormat",            "description": "<p>Please specify a format to convert to</p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "InternalServerError",            "description": "<p>Oops, we couldn't convert that PDF, please try again</p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "UnsupportedFromFormat",            "description": "<p>Sorry, we don't currently support <code>from</code></p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "UnsupportedToFormat",            "description": "<p>Sorry, we don't currently support <code>to</code></p> "          }        ]      },      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 400 Bad Request\n{\n  \"code\": 400,\n  \"error\": \"InvalidFromFormat\",\n  \"message\": \"Please specify a format to convert from\"\n}",          "type": "json"        }      ]    },    "version": "0.0.0",    "filename": "./app.js",    "groupTitle": "Convert"  }]